class arithmetic operations{
	
	public static void main(String args[])
	{
		int a=10;
		int b=2;
		int sum=a+b;
		System.out.println(sum);

		int sub=a-b;
		System.out.println(sub);

		int diff=a-b;
		System.out.println(diff);

		int mul=a*b;
		System.out.println(mul);

		int div=a/b;
		System.out.print(div);
	}
}
